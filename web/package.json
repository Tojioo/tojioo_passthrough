{
  "name": "tojioo-passthrough",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node -e \"const v=require('fs').readFileSync('../VERSION','utf8').trim();const p=require('./package.json');p.version=v;require('fs').writeFileSync('./package.json',JSON.stringify(p,null,2)+'\\n')\"",
    "prebuild": "node -e \"const v=require('fs').readFileSync('../VERSION','utf8').trim();const p=require('./package.json');p.version=v;require('fs').writeFileSync('./package.json',JSON.stringify(p,null,2)+'\\n')\"",
    "dev": "vite build --watch --config vite.config.mts",
    "build": "vite build --config vite.config.mts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@comfyorg/comfyui-frontend-types": "^1.37.2",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vitest": "^4.0.16"
  }
}